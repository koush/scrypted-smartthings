{
   "name": "@scrypted/smartthings",
   "version": "0.1.21",
   "description": "SmartThings Device Bridge for Scrypted",
   "author": "Scrypted",
   "license": "Apache",
   "scripts": {
      "prepublishOnly": "NODE_ENV=production scrypted-webpack",
      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-webpack": "scrypted-webpack"
   },
   "keywords": [
      "smartthings",
      "scrypted",
      "plugin"
   ],
   "scrypted": {
      "name": "SmartThings Bridge",
      "type": "Device",
      "singleInstance": true,
      "interfaces": [
         "DeviceProvider",
         "HttpRequestHandler"
      ]
   },
   "dependencies": {
      "axios": "^0.21.1",
      "router": "^1.3.3"
   },
   "devDependencies": {
      "@scrypted/sdk": "0.0.37"
   }
}
